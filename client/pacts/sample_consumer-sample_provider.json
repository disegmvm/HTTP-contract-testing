{
  "consumer": {
    "name": "Sample Consumer"
  },
  "provider": {
    "name": "Sample Provider"
  },
  "interactions": [
    {
      "description": "A GET request to provider",
      "providerState": "Valid request",
      "request": {
        "method": "GET",
        "path": "/cars",
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "matchingRules": {
          "$.path": {
            "match": "regex",
            "regex": "\\/cars"
          },
          "$.headers.Content-Type": {
            "match": "regex",
            "regex": "application\\/json"
          }
        }
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "body": {
          "id": "any number as string",
          "title": "any string",
          "color": "any color"
        },
        "matchingRules": {
          "$.headers.Content-Type": {
            "match": "regex",
            "regex": "application\\/json"
          },
          "$.body.id": {
            "match": "regex",
            "regex": "\\w+"
          },
          "$.body.title": {
            "match": "regex",
            "regex": "\\w+"
          },
          "$.body.color": {
            "match": "regex",
            "regex": "\\w+"
          }
        }
      }
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "2.0.0"
    }
  }
}